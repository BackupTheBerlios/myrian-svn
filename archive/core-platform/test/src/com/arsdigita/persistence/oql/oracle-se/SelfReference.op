         do {
 select parent__objects.id as parent__objects__id,
        parent__objects.type as parent__objects__type,
        parent__query_tests.name as parent__query_tests__name,
        parent__query_tests.parent_id as prnt__qry_tsts__prnt_id,
        parent_parent__objects.id as parent_parent__objects__id,
        parent_parent__objects.type as parent_parent__objects__type,
        parent_parent__query_tests.name as prnt_prnt__qry_tsts__nm,
        query_tests.id as id
   from objects parent__objects,
        objects parent_parent__objects,
        query_tests parent__query_tests,
        query_tests parent_parent__query_tests,
        query_tests query_tests
  where parent__query_tests.id = parent__objects.id(+)
    and parent__query_tests.parent_id = parent_parent__query_tests.id(+)
    and parent_parent__query_tests.id = parent_parent__objects.id(+)
    and query_tests.parent_id = parent__query_tests.id(+)
        } map {
            id = query_tests.id;
            parent.name = parent__query_tests.parent__query_tests__name;
            parent.type = parent__objects.parent__objects__type;
            parent.parent_id = parent__query_tests.prnt__qry_tsts__prnt_id;
            parent.id = parent__objects.parent__objects__id;
            parent.parent.name = parent_parent__query_tests.prnt_prnt__qry_tsts__nm;
            parent.parent.type = parent_parent__objects.parent_parent__objects__type;
            parent.parent.id = parent_parent__objects.parent_parent__objects__id;
        }
