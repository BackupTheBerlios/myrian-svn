        do {
 select acs_objects.default_domain_class as cs_objcts__dflt_dmn_clss,
        acs_objects.display_name as acs_objects__display_name,
        acs_objects.object_id as object_id,
        acs_objects.object_type as acs_objects__object_type,
        portrait__acs_objects.object_id as prtrt__acs_objcts__objct_id,
        portrait__acs_objects.object_type as prtrt__acs_objcts__objct_typ,
        portrait__docs_resources.description as prtrt__dcs_rsrcs__dscrptn,
        up_profiles.bio as up_profiles__bio,
        up_profiles.skills as up_profiles__skills,
        up_profiles.user_id as user__up_profiles__user_id
   from acs_objects acs_objects,
        acs_objects portrait__acs_objects,
        docs_resources portrait__docs_resources,
        up_profiles up_profiles
  where acs_objects.object_id = up_profiles.profile_id
    and portrait__docs_resources.resource_id = portrait__acs_objects.object_id(+)
    and up_profiles.portrait_id = portrait__docs_resources.resource_id(+)
 } map {
            displayName = acs_objects.acs_objects__display_name;
            objectType = acs_objects.acs_objects__object_type;
            skills = up_profiles.up_profiles__skills;
            bio = up_profiles.up_profiles__bio;
            defaultDomainClass = acs_objects.cs_objcts__dflt_dmn_clss;
            id = acs_objects.object_id;
            user.id = up_profiles.user__up_profiles__user_id;
            portrait.objectType = portrait__acs_objects.prtrt__acs_objcts__objct_typ;
            portrait.description = portrait__docs_resources.prtrt__dcs_rsrcs__dscrptn;
            portrait.id = portrait__acs_objects.prtrt__acs_objcts__objct_id;
        }
