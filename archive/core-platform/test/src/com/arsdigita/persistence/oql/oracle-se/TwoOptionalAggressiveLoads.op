select ao.object_id as column0,
       ao.default_domain_class as column1,
       thumbnail__ao.object_id as column2,
       portrait__ao.object_type as column3,
       portrait__ao.object_id as column4,
       tup.bio as column5,
       user__ao.object_id as column6,
       tup.skills as column7,
       ao.object_type as column8,
       ao.display_name as column9,
       portrait__tdr.description as column10
from t_up_profiles tup,
     acs_objects ao,
     acs_objects thumbnail__ao,
     acs_objects portrait__ao,
     acs_objects user__ao,
     t_docs_resources portrait__tdr
where ao.object_id = tup.profile_id and thumbnail__ao.object_id(+) = tup.thumbnail_id and portrait__ao.object_id(+) = tup.portrait_id and user__ao.object_id = tup.user_id and portrait__tdr.resource_id(+) = tup.portrait_id
