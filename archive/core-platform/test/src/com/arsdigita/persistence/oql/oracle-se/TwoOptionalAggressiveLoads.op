select tup.profile_id as c_1,
       tup.bio as c_2,
       portrait__ao.object_type as c_3,
       tup.skills as c_4,
       ao.object_type as c_5,
       tup.user_id as c_6,
       ao.default_domain_class as c_7,
       tup.portrait_id as c_8,
       portrait__tdr.description as c_9,
       ao.display_name as c_10
from t_up_profiles tup,
     acs_objects portrait__ao,
     acs_objects ao,
     t_docs_resources portrait__tdr
where portrait__ao.object_id(+) = tup.portrait_id and ao.object_id = tup.profile_id and portrait__tdr.resource_id(+) = tup.portrait_id
